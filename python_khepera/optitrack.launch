
<launch>

<!--	 
		Here add all the instances of optitrack objects to be tracked.
		The node name must match the name on the optitrack server.
		The numeric id parameter is equivalent to the Vicon id under the old system
		and should be unique.
-->	
	<node name="K8" pkg="optitrack_driver" type = "optitrack">
		<param name="vrpn_server_ip" value="192.168.2.145" />
		<param name="numeric_id" value="8" />
	</node>

	<node name="K10" pkg="optitrack_driver" type = "optitrack">
		<param name="vrpn_server_ip" value="192.168.2.145" />
		<param name="numeric_id" value="10" />
	</node>


	<node name="K7" pkg="optitrack_driver" type = "optitrack">
		<param name="vrpn_server_ip" value="192.168.2.145" />
		<param name="numeric_id" value="7" />
	</node>


	<node name="K11" pkg="optitrack_driver" type = "optitrack">
		<param name="vrpn_server_ip" value="192.168.2.145" />
		<param name="numeric_id" value="11" />
	</node>


	<node name="K13" pkg="optitrack_driver" type = "optitrack">
		<param name="vrpn_server_ip" value="192.168.2.145" />
		<param name="numeric_id" value="13" />
	</node>
</launch>
	
